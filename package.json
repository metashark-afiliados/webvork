  },
  "bugs": {
    "url": "https://github.com/metashark-afiliados/webvork/issues"
  },
  "scripts": {
    "build:i18n": "tsx --tsconfig tsconfig.json scripts/generation/build-i18n-dictionaries.ts",
    "test:smoke": "tsx --tsconfig tsconfig.json scripts/diag/smoke-test-ui.ts",
    "build": "pnpm test:smoke && pnpm build:i18n && next build",
    "dev": "next dev",
    "start": "next start",
    "lint": "eslint .",
    "format:write": "prettier --write . --log-level warn",
    "format:check": "prettier --check . --log-level warn",
    "check-types": "tsc --noEmit",
    "validate": "pnpm lint && pnpm format:check && pnpm check-types",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "gen:icons": "tsx --tsconfig tsconfig.json scripts/generation/generate-lucide-icon-enum.ts",
    "gen:routes": "tsx --tsconfig tsconfig.json scripts/generation/generate-navigation-manifest.ts",
    "generate": "pnpm gen:icons && pnpm gen:routes"
  },
  "dependencies": {
    "@devnomic/marquee": "^1.0.2",
